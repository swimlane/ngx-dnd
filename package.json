{
  "name": "@swimlane/ngx-dnd",
  "version": "4.0.0",
  "description": "Drag and Drop for Angular2 and beyond!",
  "main": "./release/bundle/ngx-dnd.umd.js",
  "module": "./release/lib/index.js",
  "typings": "./release/lib/index.d.ts",
 "files": [
    "release"
  ],
  "scripts": {
    "check": "npm-check --skip-unused ",
    "test": "npm run lint && karma start",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "lint": "tslint \"./src/{,**/}*.ts\" \"./demo/{,**/}*.ts\"",
    "version": "chg release -y && git add -A CHANGELOG.md && npm run release",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:release": "rimraf release",
    "copy:html": "cpx \"src/**/*.html\" release/lib/",
    "build": "webpack --display-error-details",
    "build:sass": "node-sass -r src/ -o release/lib/ --source-map=true",
    "build:release": "cross-env NODE_ENV=production npm run build",
    "build:package": "cross-env NODE_ENV=package npm run build",
    "build:inline": "ng2-inline --compress -b release/lib/ -o ./ \"release/lib/**/*.js\" --relative",
    "build:css": "postcss --use autoprefixer release/**/*.css -d release/ ",
    "watch": "webpack --display-error-details --watch",
    "start": "webpack-dev-server",
    "start:hmr": "webpack-dev-server --env.HMR",
    "release": "npm-run-all build:release",
    "np": "npm run package && np --yolo",
    "package": "npm-run-all -s clean package:aot copy:html build:sass package:replace build:inline build:package ",
    "package:aot": "ngc -p tsconfig-aot.json",
    "package:replace": "node ./config/replace.js",
    "package:replace-scss": "node ./config/replace-scss.js",
    "deploy": "npm run build && node ./config/deploy.js",
    "profile": "NODE_ENV=production webpack --profile --json > dist/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swimlane/ngx-dnd.git"
  },
  "keywords": [
    "angular2",
    "angular4",
    "angular5",
    "dnd",
    "drag",
    "drop",
    "angular"
  ],
  "authors": [
    "Jayson Harshbarger"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swimlane/ngx-dnd/issues"
  },
  "homepage": "https://github.com/swimlane/ngx-dnd#readme",
  "peerDependencies": {
    "@angular/animations": "~4.4.0 || ^5.x",
    "@angular/common": "~4.4.0 || ^5.x",
    "@angular/compiler": "~4.4.0 || ^5.x",
    "@angular/core": "~4.4.0 || ^5.x",
    "@angular/platform-browser": "~4.x || ^5.x",
    "@angular/platform-browser-dynamic": "~4.x || ^5.x",
    "core-js": "^2.4.1",
    "rxjs": "^5.4.0",
    "zone.js": "^0.8.0"
  },
  "devDependencies": {
    "@angular/animations": "^5.1.0",
    "@angular/common": "^5.1.0",
    "@angular/compiler": "^5.1.0",
    "@angular/compiler-cli": "^5.1.0",
    "@angular/core": "^5.1.0",
    "@angular/flex-layout": "^2.0.0-beta.10-4905443",
    "@angular/forms": "^5.1.0",
    "@angular/http": "^5.1.0",
    "@angular/platform-browser": "^5.1.0",
    "@angular/platform-browser-dynamic": "^5.1.0",
    "@angular/platform-server": "^5.1.0",
    "@ngtools/webpack": "^1.6.0",
    "@swimlane/ngx-ui": "^19.1.0",
    "@types/core-js": "^0.9.41",
    "@types/jasmine": "^2.5.53",
    "@types/dragula": "^2.1.32",
    "@types/node": "^8.0.58",
    "angular2-inline-template-style": "^1.2.0",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^7.2.2",
    "awesome-typescript-loader": "~3.4.1",
    "chg": "^0.3.2",
    "clean-webpack-plugin": "^0.1.15",
    "codelyzer": "^4.0.2",
    "copy-webpack-plugin": "^4.0.0",
    "core-js": "2.5.3",
    "cpx": "^1.5.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "fs-extra": "^5.0.0",
    "gh-pages": "^1.1.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.26.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "node-sass": "^4.5.3",
    "np": "^2.16.0",
    "npm-check": "^5.4.5",
    "npm-run-all": "^4.0.1",
    "postcss": "^6.0.14",
    "postcss-loader": "^2.0.9",
    "raw-loader": "^0.5.1",
    "replace": "^0.3.0",
    "rimraf": "^2.5.2",
    "rxjs": "^5.4.3",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.3",
    "string-replace-loader": "^1.3.0",
    "style-loader": "^0.19.0",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "^1.1.2",
    "tslint": "^5.6.0",
    "tslint-config-swimlane": "^3.0.2",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.4.2",
    "uglify-js": "^3.2.2",
    "url-loader": "^0.6.2",
    "webpack": "3.10.0",
    "webpack-combine-loaders": "^2.0.3",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0",
    "webpack-notifier": "^1.5.0",
    "zone.js": "^0.8.16"
  },
  "dependencies": {
    "dragula": "^3.7.2",
    "@types/dragula": "^2.1.32"
  }
}
