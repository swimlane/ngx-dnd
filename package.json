{
  "name": "ngx-dnd",
  "version": "5.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "serve": "ng serve",
    "start": "npm-run-all build:lib serve",
    "start:prod": "npm-run-all build:prod:lib serve",
    "test": "start-server-and-test serve http-get://localhost:4200 cy:open",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:single": "start-server-and-test serve http-get://localhost:4200 cy:run",
    "lint": "ng lint",
    "prettier": "prettier --write \"**/*.{js,ts}\"",
    "changelog": "chg release -y && git add -A CHANGELOG.md",
    "preversion": "npm run test:single",
    "version": "npm run changelog",
    "postversion": "cpx CHANGELOG.md ./projects/swimlane/ngx-dnd",
    "prepublish:lib": "npm-run-all build:prod:lib test:single",
    "publish:lib": "cd dist/swimlane/ngx-dnd && npm publish --tag next",
    "build:lib": "ng build @swimlane/ngx-dnd",
    "build:prod:lib": "ng build --prod @swimlane/ngx-dnd",
    "pack:lib": "cd dist/swimlane/ngx-dnd && npm pack",
    "prepublishOnly": "echo \"This is the workspace -- you probably meant run 'npm run lib:prepublish && npm run lib:publish' from dist/swimlane/ngx-dnd.\" && exit 1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.2",
    "@angular/cdk": "^6.1.0",
    "@angular/common": "^6.0.2",
    "@angular/compiler": "^6.0.2",
    "@angular/core": "^6.0.2",
    "@angular/flex-layout": "^6.0.0-beta.15",
    "@angular/forms": "^6.0.2",
    "@angular/http": "^6.0.2",
    "@angular/platform-browser": "^6.0.2",
    "@angular/platform-browser-dynamic": "^6.0.2",
    "@angular/router": "^6.0.2",
    "@swimlane/dragula": "^3.7.3",
    "@swimlane/ngx-ui": "^21.1.2",
    "@types/dragula": "^2.1.33",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.2.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.3",
    "@angular-devkit/build-ng-packagr": "~0.6.3",
    "@angular/cli": "~6.0.3",
    "@angular/compiler-cli": "^6.0.2",
    "@angular/language-service": "^6.0.2",
    "@swimlane/prettier-config-swimlane": "^0.1.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.9.5",
    "chg": "^0.3.3",
    "codelyzer": "~4.2.1",
    "cpx": "^1.5.0",
    "cypress": "^3.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0-rc.2",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.12.1",
    "protractor": "~5.3.0",
    "start-server-and-test": "^1.4.1",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "tslint-config-prettier": "^1.13.0",
    "tslint-config-swimlane": "^3.0.4",
    "typescript": "~2.7.2"
  }
}
